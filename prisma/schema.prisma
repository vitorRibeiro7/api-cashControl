// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model transaction {
  id          Int     @id @default(autoincrement())
  description String
  price       Int
  category    String
  type        Boolean @default(true)

  @@map("Transactions")
}
